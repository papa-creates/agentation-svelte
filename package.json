    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "publint": "^0.2.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "cookie": "^1.1.1",
