{
  "name": "agentation-svelte",
  "version": "1.0.0",
  "description": "Visual feedback for AI coding agents - Svelte version",
  "svelte": "./src/lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/lib/index.js"
    }
